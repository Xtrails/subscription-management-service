<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">
    <!--
        Added the constraints for entity ClientSubscription.
    -->
    <changeSet id="20241012075804-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="user_id"
                                 baseTableName="client_subscription"
                                 constraintName="fk_client_subscription__user_id"
                                 referencedColumnNames="id"
                                 referencedTableName="external_user"
                                 />

        <addForeignKeyConstraint baseColumnNames="subscription_type_id"
                                 baseTableName="client_subscription"
                                 constraintName="fk_client_subscription__subscription_type_id"
                                 referencedColumnNames="id"
                                 referencedTableName="subscription_type"
                                 />

        <addForeignKeyConstraint baseColumnNames="source_application_id"
                                 baseTableName="client_subscription"
                                 constraintName="fk_client_subscription__source_application_id"
                                 referencedColumnNames="id"
                                 referencedTableName="source_application"
                                 />
    </changeSet>
</databaseChangeLog>
